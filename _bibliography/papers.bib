
@misc{alice_collaboration_letter_2020,
	title = {Letter of {Intent}: {A} {Forward} {Calorimeter} ({FoCal}) in the {ALICE} experiment},
	copyright = {All rights reserved},
	shorttitle = {Letter of {Intent}},
	url = {https://cds.cern.ch/record/2719928},
	abstract = {We propose a forward electromagnetic and hadronic calorimeter (FoCal) as an upgrade to the ALICE experiment, to be installed during LS3 for data-taking in 2027–2029 at the LHC. The FoCal is a highly granular Si+W electromagnetic calorimeter combined with a conventional sampling hadronic calorimeter covering pseudorapidities of 3.4 \&lt; ${\textbackslash}eta$ \&lt; 5.8. The FoCal provides unique capabilities to measure small-x gluon distributions via prompt photon production and will significantly enhance the scope of ALICE for inclusive and correlation measurements with mesons, photons, and jets to explore the dynamics of hadronic matter at small x down to {\textasciitilde}$10{\textasciicircum}\{ -6\}$},
	language = {en},
	urldate = {2021-11-22},
	journal = {CERN Document Server},
	author = {ALICE Collaboration},
	month = jun,
	year = {2020},
	note = {Number: CERN-LHCC-2020-009},
	keywords = {⛔ No INSPIRE recid found},
	file = {ALICE Collaboration_2020_Letter of Intent.pdf:/Users/florianjonas/Zotero/storage/QEF4ZHW4/QEF4ZHW4.pdf:application/pdf;Snapshot:/Users/florianjonas/Zotero/storage/543YKY5L/2719928.html:text/html},
	category = {preprint},
	abbr = {Report},
	inspire = {https://inspirehep.net/literature/1805025},
}

@article{ALICE:2020ylo,
    author = "Acharya, Shreyasi and others",
    collaboration = "ALICE",
    title = "{Production of \(\omega\)~mesons in pp collisions at \(\sqrt\{s\}=\)7 TeV}",
	abstract = {The invariant differential cross section of inclusive ${\textbackslash}omega(782)$ meson production at midrapidity (${\textbar}y{\textbar}{\textless}0.5$) in pp collisions at ${\textbackslash}sqrt\{s\}$ = 7 TeV was measured with the ALICE detector at the LHC over a transverse momentum range of 2 {\textless} $p\_\{{\textbackslash}rm\{T\}\}$ {\textless} 17 GeV/$c$. The ${\textbackslash}omega$ meson was reconstructed via its ${\textbackslash}omega{\textbackslash}rightarrow{\textbackslash}pi{\textasciicircum}+{\textbackslash}pi{\textasciicircum}-{\textbackslash}pi{\textasciicircum}0$ decay channel. The measured ${\textbackslash}omega$ production cross section is compared to various calculations: PYTHIA 8.2 Monash 2013 describes the data, while PYTHIA 8.2 Tune 4C overestimates the data by about 50\%. A recent NLO calculation, which includes a model describing the fragmentation of the whole vector-meson nonet, describes the data within uncertainties below 6 GeV/$c$, while it overestimates the data by up to 50\% for higher $p\_\{{\textbackslash}rm\{T\}\}$. The ${\textbackslash}omega/{\textbackslash}pi{\textasciicircum}0$ ratio is in agreement with previous measurements at lower collision energies and the PYTHIA calculations. In addition, the measurement is compatible with transverse mass scaling within the measured $p\_\{{\textbackslash}rm\{T\}\}$ range and the ratio is constant with $C{\textasciicircum}\{{\textbackslash}omega/{\textbackslash}pi{\textasciicircum}\{0\}\}$ = 0.67 ${\textbackslash}pm$ 0.03 (stat) ${\textbackslash}pm$ 0.04 (sys) above a transverse momentum of 2.5 GeV/$c$.},
    eprint = "2007.02208",
    archivePrefix = "arXiv",
    primaryClass = "nucl-ex",
    reportNumber = "CERN-EP-2020-122",
    doi = "10.1140/epjc/s10052-020-08651-y",
    journal = "Eur. Phys. J. C",
    volume = "80",
    number = "12",
    pages = "1130",
    year = "2020",
	selected = {true},
	abbr = {Eur. Phys. J. C},
	altmetric = true,
	inspire = {https://inspirehep.net/literature/1805263},
}

@article{Jonas:2021xju,
	title = {Centrality dependence of electroweak boson production in {PbPb} collisions at the {LHC}},
	volume = {104},
	copyright = {All rights reserved},
	arxiv = {2104.14903},
	doi = {10.1103/PhysRevC.104.044905},
	inspire = {https://inspirehep.net/literature/1861559}
	abstract = {Recent data on the nuclear modification of W and Z boson production measured by the ATLAS collaboration in PbPb collisions at \({\textbackslash}sqrt\{s\_\{{\textbackslash}rm nn\}\}=5.02\) TeV show an enhancement in peripheral collisions, seemingly contradicting predictions of the Glauber model. The data were previously explained by arguing that the nucleon-nucleon cross section may be shadowed in nucleus-nucleus collisions, and hence suppressed compared to the proton-proton cross section at the same collision energy. This interpretation has quite significant consequences for the understanding of heavy-ion data, in particular in the context of the Glauber model. Instead, we provide an alternative explanation of the data by assuming that there is a mild bias present in the centrality determination of the measurement; on the size of the related systematic uncertainty. Using this assumption, we show that the data is in agreement with theoretical calculations using nuclear parton distribution functions. Finally, we speculate that the centrality dependence of the W${\textasciicircum}-$/W${\textasciicircum}\{+\}$ ratio may point to the relevance of a larger skin thickness of the Pb nucleus, which, if present, would result in a few percent larger PbPb cross section than currently accounted for in the Glauber model and may hence be the root of the centrality bias.},
	number = {4},
	urldate = {2021-10-13},
	journal = {Phys. Rev. C},
	abbr = {Phys. Rev. C},
	author = {Jonas, Florian and Loizides, Constantin},
	month = oct,
	year = {2021},
	keywords = {High Energy Physics - Phenomenology, Nuclear Experiment, Nuclear Theory},
	pages = {044905},
	file = {arXiv Fulltext PDF:/Users/florianjonas/Zotero/storage/TCQ9P3EL/Jonas and Loizides - 2021 - Centrality dependence of electroweak boson product.pdf:application/pdf;arXiv.org Snapshot:/Users/florianjonas/Zotero/storage/GIN5NQ35/2104.html:text/html},
	selected = {true},
	altmetric = {105142015},
}

