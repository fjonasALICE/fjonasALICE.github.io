@misc{noauthor_physics_2023,
	title = {Physics of the {ALICE} {Forward} {Calorimeter} upgrade},
	copyright = {All rights reserved},
	author = {ALICE collaboration},
	abbr = {Public Note},
	url = {https://cds.cern.ch/record/2858858},
	note = {ALICE-PUBLIC-2023-001},
	abstract = {The ALICE Collaboration proposes to instrument the existing ALICE detector with a forward calorimeter system (FoCal), planned to take data during LHC Run 4 (2029$-$2032). The FoCal detector is a highly-granular Si+W electromagnetic calorimeter combined with a conventional sampling hadronic calorimeter, covering the pseudorapidity interval of $3.4\&lt;{\textbackslash}eta\&lt;5.8$.  The FoCal design is optimized to measure isolated photons at most forward rapidity for $p\_\{{\textbackslash}rm T\}{\textbackslash}gtrsim4$ GeV/$c$.  In this note we discuss the scientific potential of FoCal, which will enable broad exploration of gluon dynamics and non-linear QCD evolution at the smallest values of Bjorken $x$ accessible at any current or near-future facility world-wide. FoCal will measure theoretically well-motivated observables in pp and p$-$Pb collisions which are sensitive to the gluon distribution at small $x$ at low to moderate $Q{\textasciicircum}2$, based on isolated photon, neutral meson, and jet production and correlations in hadronic collisions, and the measurement of vector meson photoproduction in ultra-peripheral collisions. These FoCal measurements will provide incisive tests of the universality of linear and non-linear QCD evolution in different collision systems over an unprecedented  kinematic range, in particular when combined with the comprehensive experimental program at the EIC and other forward measurements at RHIC and the LHC. FoCal will also carry out measurements at very forward rapidity in Pb$-$Pb collisions, enabling novel probes of the Quark-Gluon Plasma based on jet quenching phenomena and long-range correlations of neutral pions, jets, and photons.},
	urldate = {2023-11-08},
	year = {2023},
	keywords = {⛔ No INSPIRE recid found},
	file = {Full Text PDF:/Users/florianjonas/Zotero/storage/23FNYRYL/2023 - Physics of the ALICE Forward Calorimeter upgrade.pdf:application/pdf},
	inspire = {https://inspirehep.net/literature/2661418},
}

@article{Aehle:2023mpy,
	title = {Performance of the electromagnetic and hadronic prototype segments of the {ALICE} {Forward} {Calorimeter}},
	copyright = {All rights reserved},
	url = {http://arxiv.org/abs/2311.07413},
	abstract = {We present the performance of a full-length prototype of the ALICE Forward Calorimeter (FoCal). The detector is composed of a silicon-tungsten electromagnetic sampling calorimeter with longitudinal and transverse segmentation (FoCal-E) of about 20$X\_0$ and a hadronic copper-scintillating-fiber calorimeter (FoCal-H) of about 5${\textbackslash}lambda\_\{{\textbackslash}rm int\}$. The data were taken between 2021 and 2023 at the CERN PS and SPS beam lines with hadron (electron) beams up to energies of 350 (300) GeV. Regarding FoCal-E, we report a comprehensive analysis of its response to minimum ionizing particles across all pad layers. The longitudinal shower profile of electromagnetic showers is measured with a layer-wise segmentation of 1$X\_0$. As a projection to the performance of the final detector in electromagnetic showers, we demonstrate linearity in the full energy range, and show that the energy resolution fulfills the requirements for the physics needs. Additionally, the performance to separate two-showers events was studied by quantifying the transverse shower width. Regarding FoCal-H, we report a detailed analysis of the response to hadron beams between 60 and 350 GeV. The results are compared to simulations obtained with a Geant4 model of the test beam setup, which in particular for FoCal-E are in good agreement with the data. The energy resolution of FoCal-E was found to be lower than 3\% at energies larger than 100 GeV. The response of FoCal-H to hadron beams was found to be linear, albeit with a significant intercept that is about factor 2 larger than in simulations. Its resolution, which is non-Gaussian and generally larger than in simulations, was quantified using the FWHM, and decreases from about 16\% at 100 GeV to about 11\% at 350 GeV. The discrepancy to simulations, which is particularly evident at low hadron energies, needs to be further investigated.},
	urldate = {2023-11-14},
	author = {Aehle, M. and Alme, J. and Arata, C. and Arsene, I. and Bearden, I. and Bodova, T. and Borshchov, V. and Bourrion, O. and Bregant, M. and Brink, A. van den and Buchakchiev, V. and Buhl, A. and Chujo, T. and Dufke, L. and Eikeland, V. and Fasel, M. and Gauger, N. and Gautam, A. and Ghimouz, A. and Goto, Y. and Guernane, R. and Hachiya, T. and Hassan, H. and He, L. and Helstrup, H. and Huhta, L. and Inaba, M. and Isidori, T. and Jonas, F. and Kawaguchi, T. and Keidel, R. and Kim, M. and Kozhuharov, V. and Kumaoka, T. and Kusch, L. and Loizides, C. and Melikyan, Y. and Miake, Y. and Minafra, N. and Nystrand, J. and Novitzky, N. and Øekland, T. and Oyama, K. and Park, H. and Park, J. and Pascal, I. and Peitzmann, T. and Protsenko, M. and Räsänen, S. S. and Rarbi, F. and Rauch, M. and Rehman, A. and Richter, M. and Röhrich, D. and Røed, K. and Rusu, A. and Rytkönen, H. and Sakai, S. and Sato, K. and Schilling, A. and Shimizu, S. and Shimomura, M. and Simeonov, R. and Solheim, E. and Sugitate, T. and Takaki, D. Tapai and Tourres, D. and Tymchuk, I. and Yi, J. and Yin, Z. and Ullaland, K. and Yang, S. and Yokoo, T. and Zhou, D. and Zillien, S.},
	month = nov,
	year = {2023},
	abbr = {preprint},
	inspire = {https://inspirehep.net/literature/2722124},
}

